LEXICON ORD
+Ord: NumCASEPOSSLEX ; ! ӧт part of the stem
+Ord+Iter:%>%{иі%}сь K ;
+Ord+Iter+Distr:%>%{иі%}сьӧн K ;!CHECKME 2019-11-04

LEXICON NUM-IS_DISTR
+Distr: K ;

LEXICON QNT_
: NumCASEPOSSLEX ;
