LEXICON PRONOUN-TYPES
 PERS ;
! INTERR ;
! INDEFINITE-PRON ;
! NEG-PRON ;
! COLL-PRON ;


LEXICON PERS                        !!≈ * __@CODE@__ 
ме+Pron+Pers+Sg1:ме perssg1decl ;    !!≈ * __@CODE@__ ...
тэ+Pron+Pers+Sg2:ті perssg2decl ; 
сія+Pron+Pers+Sg3:с perssg3decl ;
ме+Pron+Pers+Pl1:ми perspl1decl ;
тэ+Pron+Pers+Pl2:ті perspl2decl ; 
сія+Pron+Pers+Pl3:н perspl3decl ;

LEXICON PERS-SG1_
+Pron+Pers+Sg1: perssg1decl ;
LEXICON PERS-SG2_
+Pron+Pers+Sg2: perssg2decl ;
LEXICON PERS-SG3_
+Pron+Pers+Sg3: perssg3decl ;

LEXICON PERS-PL1_
+Pron+Pers+Pl1: perspl1decl ;
LEXICON PERS-PL2_
+Pron+Pers+Pl2: perspl2decl ;
LEXICON PERS-PL3_
+Pron+Pers+Pl3: perspl3decl ;

LEXICON perssg1decl   !!≈ * __@CODE@__ 
+Nom: WordEnd ;
: GEN_лӧн-THEN-K ;
+Ela+PxSg1:нсьым WordEnd ; !The elative case is used in Sg1 and Sg2 
                 !for the functions of the ablative in other persons
+Dat:ным WordEnd ; !What should be done with the 4 dative forms?
+Dat:м WordEnd ;
+Dat:н WordEnd ;
:н DAT_лӧ-THEN-K ;
+Acc:нӧ WordEnd ;

:нам WordEnd ;
:нам GEN_лӧн-THEN-K ;
+Ins:намӧн WordEnd ;
+Com:намкӧт WordEnd ;
:нам CAR_тӧг-THEN-K ;
+Inе:намын WordEnd ;
:нам ELA_ісь-THEN-K ;
+Ill:намӧ WordEnd ;
:нам APR_сянь-THEN-K ;
+Ter:намӧдз WordEnd ;
: perssg ; ! for the regular obl sg

LEXICON perspl1decl    !!≈ * __@CODE@__
+Nom: WordEnd ;
+Nom:йӧ WordEnd ;
:ян WordEnd ;
+Acc:янӧс WordEnd ;
:ян persplobl ;
!!Extras form 2000 Grammar: 163
:ян GEN_лӧн-THEN-K ; !Synomymous with: миян
+PxPl1+Acc:яннымӧс WordEnd ; !Synomymous with: миянӧс
+Ela+PxPl1:янсьыным WordEnd ; !Synomymous with:миянлысь
+Com:кӧт WordEnd ; !Synomymous with: миянкӧт
+PxPl1+Com:яннымкӧт WordEnd ; !Synomymous with: миянкӧт
: CAR_тӧг-THEN-K ; !Synomymous with: миянтӧг
+Car+PxPl1:янтӧгным WordEnd ; !Synomymous with: миянтӧг
+PxPl1+Cns:яннымла WordEnd ; !Synomymous with: миянла
: ELA_ісь-THEN-K ; !Synomymous with: миянысь
+Ill:ӧ WordEnd ; !Synomymous with: миянӧ
+Apr+PxPl1:янланьным WordEnd ; !Synomymous with: миянлань
+Apr+PxPl1:ланьным WordEnd ; !Synomymous with: миянлань
+Egr+PxPl1:янсяньным WordEnd ; !Synomymous with: миянсянь
+TerI:ӧдзз WordEnd ; !Synomymous with: миянӧдз

LEXICON perssg2decl     !!≈ * __@CODE@__
+Nom: WordEnd ;
+Gen:нат WordEnd ;
+Dat:ныт WordEnd ;
 GEN_лӧн-THEN-K ;
+Ela+PxSg2:нсьыт WordEnd ;
+Dat:н WordEnd ;
+Dat:т WordEnd ;
:т DAT_лӧ-THEN-K ;
:н DAT_лӧ-THEN-K ;
:ныт DAT_лӧ-THEN-K ;
+Acc:нӧ WordEnd ;
:нат WordEnd ;
:нат GEN_лӧн-THEN-K ;
+Ins:натӧн WordEnd ;
+Com:накӧт WordEnd ;
:на CAR_тӧг-THEN-K ;
+Ine:натын WordEnd ;
:нат ELA_ісь-THEN-K ;
+Ill:натӧ WordEnd ;
+Egr:натсянь WordEnd ;
+Ter:натӧдз WordEnd ;
: perssg ;  ! for the regular obl sg

LEXICON perspl2decl !!≈ * __@CODE@__ 
+Nom: WordEnd ;
+Der/йӧ+Nom:йӧ WordEnd ; !We need a special tag for discerning +Nom forms
:ян DAT_лӧ-THEN-K ;

:ян WordEnd ;
:ян GEN_лӧн-THEN-K ;
+Acc:янӧс WordEnd ;
:ян persplobl ;

!More forms from Ӧнія коми кыв 2000: 163
+Ins:ӧн WordEnd ;
CAR_тӧг-THEN-K ;
ELA_ісь-THEN-K ;
+Ill:ӧ WordEnd ;
+Abl+PxPl2:янлысьныд WordEnd ;
+PxPl2+PxSg2+Acc2:янныдтӧ WordEnd ;
+PxPl2+Car:янныдтӧг WordEnd ;
+PxPl2+Cns:янныдла WordEnd ;
+Apr+PxPl2:янланьныд WordEnd ;
+Apr+PxSg2:янланьыд WordEnd ;
+Egr+PxPl2:янсяньныд WordEnd ;
+Ter:ӧдз WordEnd ;

LEXICON perssg3decl  !!≈ * __@CODE@__ 
+Nom:ія WordEnd ;
+Der/йӧ+Acc:ійӧ WordEnd ;

:ы GEN_лӧн-THEN-K ;
+Abl:ылісь WordEnd ;
:ы ELA_ісь-THEN-K ;
:ы DAT_лӧ-THEN-K ;
+Der/йӧ+Ins:ійӧн WordEnd ;
:ы perssg ;  ! for the regular obl sg
:ы WordEnd ;
!More form ӧния коми кыв 163
+Der/йӧ:ійӧ GEN_лӧн-THEN-K ;
+Der/йӧ+Abl:ійӧлісь WordEnd ;
+Der/йӧ:ійӧ DAT_лӧ-THEN-K ;
+Der/йӧ+Com:ійӧкӧт WordEnd ;
+Der/йӧ:ійӧ CAR_тӧг-THEN-K ;
+Der/йӧ:ійӧ APR_сянь-THEN-K ;
+Der/йӧ+Egr:ійӧсянь WordEnd ;
+Der/йӧ+Egr+PxSg3:ійӧсяньыс WordEnd ;
+Der/йӧ+Ter:ійӧӧдз WordEnd ;
! Special expressions
+Com+Ins:ыкӧтӧн WordEnd ; ! Ен сыкӧтӧн!

!! ми мийӧ The 1st and 2nd persons have Oblique case stem strategies
!! that differ from the 3rd person: 
!! ті тійӧ nämä ovat aivan eri asioita
!! сы сійӧ tosin joskus 

LEXICON perspl3decl !!≈ * __@CODE@__
+Nom:ія WordEnd ;
+Der/йӧ+Acc:ійӧ WordEnd ;

+Der/йӧ+Acc:айӧс WordEnd ;
:на GEN_лӧн-THEN-K ;
:ы persplobl ; !This might be one solution for the different stem types
:ы WordEnd ;
!
+Der/йӧ:найӧ GEN_лӧн-THEN-K ;
+Der/йӧ+Abl:найӧлісь WordEnd ;
+Der/йӧ:найӧ DAT_лӧ-THEN-K ;
+Der/йӧ+Ins:найӧӧн WordEnd ;
+Der/йӧ+Com:найӧкӧт WordEnd ;
+Com:накӧтыс WordEnd ;
+Der/йӧ:найӧ CAR_тӧг-THEN-K ;
+Car+PxSg3:натӧгыс WordEnd ;
+PxSg3+Cns:наысла WordEnd ;
+Ela+PxSg3:насьыс WordEnd ;
+PxSg3+Apr:наыслань WordEnd ;
+Apr+PxSg3:наланьыс WordEnd ;
+PxSg3+Egr:наыссянь WordEnd ;
+Egr+PxSg3:насяньыс WordEnd ;
+Tra+PxSg3:натіыс WordEnd ;
+Ter+PxSg3:наӧдзыс WordEnd ;


LEXICON perssg !!≈ * __@CODE@__
+Ins:ӧн WordEnd ;
+Com:кӧт WordEnd ;
CAR_тӧг-THEN-K ;
+Cns:ла WordEnd ;
+Ine:ын WordEnd ;
+Ill:ӧ WordEnd ; !
ELA_ісь-THEN-K ;
APR_сянь-THEN-K ;
+Egr:сянь WordEnd ;
+Prl:ӧт WordEnd ;
+TerI:ӧдзз WordEnd ;
+Abl:лісь WordEnd ; !This is not attested for Sg1 and Sg2 in the 2000 Grammar



LEXICON persplobl !!≈ * __@CODE@__
+Abl:лісь WordEnd ;
DAT_лӧ-THEN-K ;
+Acc:ӧс WordEnd ;

+Ins:ӧн WordEnd ;
+Com:кӧт WordEnd ;
CAR_тӧг-THEN-K ;
+Cns:ла WordEnd ;
+Cmpr:ся WordEnd ;
+Ine:ын WordEnd ;
ELA_ісь-THEN-K ;
+Ill:ӧ WordEnd ;
APR_сянь-THEN-K ;
+Egr:сянь WordEnd ;
+Prl:ӧд WordEnd ;
+Tra:ті WordEnd ;
+Ter:ӧдз WordEnd ;
+AprIne:ланьын WordEnd ;  ! New cases in last grammar  
+AprEla:ланьысь WordEnd ;  ! 
+AprIll:ланьӧ WordEnd ; ! 
+AprEgr:ланьсянь WordEnd ; ! 
+AprPrl:ланьӧд WordEnd ; !
+AprTra:ланьті WordEnd ; !       
+AprTer:ланьӧдз WordEnd ; ! 


